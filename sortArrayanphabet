// Sort an array alphabetically
function alphabetical(arr) {
  var newArr = arr.sort(function(a,b){
    if(a<b){
      return -1;
    }
    else if(a>b){
      return 1;
    }
    else{
      return 0;
    }
  });
  return newArr;
}

alphabetical(["dog", "wolf", "by", "family", "eaten"]) 
// ["by", "dog", "eaten", "family", "wolf"] 
